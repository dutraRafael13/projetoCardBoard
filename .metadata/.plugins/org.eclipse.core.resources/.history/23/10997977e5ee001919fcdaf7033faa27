package br.com.cardboard.transportadoraAPI.repository;

import java.util.Optional;

import org.springframework.data.jpa.repository.JpaRepository;

import br.com.cardboard.transportadoraAPI.models.DadosTransportadoraModel;

public interface DadosTransportadoraRepository extends JpaRepository<DadosTransportadoraModel, Integer> {
	
	Optional<DadosTransportadoraModel> findById(Integer idTipoTransporte);

}
