public class CalcularMelhorTransportadora {
	
	private String calcula(List<TransportadoraModel> transportadoras, long distancia, EnumPrioridadeTransporte prioridade) {
		return "";	
	}
	
	private String calcula(List<TransportadoraModel> transportadoras, long distancia, EnumPrioridadeTransporte prioridade, EnumTipoTransporte tipoTransporte) {
		return "";	
	}
	
	private double calculoPrecoFinal(long distancia, double valor) {
		return (distancia * valor) / 10;
	}

